{
    email typingrealm@gmail.com
}

typingrealm.com {
    reverse_proxy typingrealm-ui:80
}

api.typingrealm.com {
    handle_path /typing/* {
        reverse_proxy typingrealm-typing-node:80 typingrealm-typing-node-2:80 typingrealm-typing-node-3:80 {
            lb_policy round_robin
            health_path /health
            health_interval 10s
        }
    }

    respond 404
}
